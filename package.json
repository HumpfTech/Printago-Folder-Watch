{
  "name": "printago-folder-watch",
  "version": "1.0.0",
  "description": "Secure directory watcher and file sync tool for Printago",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "build": "pkg . --targets node18-win-x64 --output dist/PrintagoFolderWatch.exe"
  },
  "author": "Humpf Tech LLC",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "chokidar": "^3.5.3",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "bin": "app.js",
  "pkg": {
    "assets": [
      "node_modules/axios/**/*",
      "node_modules/chokidar/**/*",
      "node_modules/form-data/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist"
  }
}
